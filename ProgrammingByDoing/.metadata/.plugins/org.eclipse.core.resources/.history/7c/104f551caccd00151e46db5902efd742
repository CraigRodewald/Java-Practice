
public class Initials {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[][] a = new String[][] {{" ", " " , "a" , " " , " "},
									   {" ", "a" , "" , "a" , " "},
									   {"a", " " , " " , " " , "a"},
									   {"a", "a" , "a" , "a" , "a"},
									   {"a", " " , " " , " " , "a"},
									   {"a", " " , " " , " " , "a"},
									   {"a", " " , " " , " " , "a"}};
									   
		for (int i = 0; i < 7; i++) {
			for (int j = 0; j < a.length; j++) {
				System.out.print(a[i][j]);
			}
			System.out.println("\n");
		}							   

	}

}
