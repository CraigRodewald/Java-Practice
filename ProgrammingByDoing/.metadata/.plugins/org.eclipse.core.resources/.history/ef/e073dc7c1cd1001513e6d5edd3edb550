
public class MultiplicationTable {

	public static void main(String[] args) {
		for (int i = 0; i < 12; i++) {
			if (i<1) {
				System.out.print("x |\t");
			}
			if (i >= 1 && i <= 12) {
				System.out.println(i);
			}
			for (int j = 0; j < 9; j++) {
				if (i<1) {
					System.out.print(j+1 + "\t");
				}
			}
		}

	}

}
